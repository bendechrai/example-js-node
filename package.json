{
  "name": "arcjet-node-express-example",
  "description": "Arcjet Node.js and Express example app. Arcjet helps developers protect their apps in just a few lines of code.",
  "license": "Apache-2.0",
  "homepage": "https://arcjet.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcjet/arcjet-node-express-example.git",
    "directory": "arcjet"
  },
  "bugs": {
    "url": "https://github.com/arcjet/arcjet-node-express-example/issues",
    "email": "support@arcjet.com"
  },
  "author": {
    "name": "Arcjet",
    "email": "support@arcjet.com",
    "url": "https://arcjet.com"
  },
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prestart": "node -e \"if (process.versions.node.split('.')[0] < 22) { console.error('This app requires Node.js 22 or higher'); process.exit(1); }\"",
    "start": "node --env-file=.env.local --experimental-require-module -r ts-node/register src/server.ts",
    "predev": "node -e \"if (process.versions.node.split('.')[0] < 22) { console.error('This app requires Node.js 22 or higher'); process.exit(1); }\"",
    "dev": "nodemon --exec 'node --env-file=.env.local --experimental-require-module -r ts-node/register' src/server.ts",
    "build": "tsc"
  },
  "dependencies": {
    "@arcjet/decorate": "^1.0.0-alpha.26",
    "@arcjet/next": "^1.0.0-alpha.26",
    "@arcjet/node": "^1.0.0-alpha.26",
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "express-session": "^1.18.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.5.5",
    "@types/passport": "^1.0.16",
    "@types/passport-github2": "^1.2.9",
    "nodemon": "^3.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}